/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
  height: 100%;
  width: 100%;
}

button,
button[type=default]{
  background: #f03d64;
  color: #fff;
}

.button-hover,
.button-hover[type=default]{
  background: #f95e80;
  color: #fff;
}

button[type=primary]{
  background: #113e6e;
  color: #fff;
}

.button-hover[type=primary]{
  background: #195696;
  color: #fff;
}

button::after{
  border: 0;
}

@import 'style/weui.wxss'
