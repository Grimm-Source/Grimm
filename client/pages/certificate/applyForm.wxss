/* pages/certificate/applyList.wxss */
@import 'applyList-wxa-auto-dark.wxss';
page{
  height: 100%;
}

.apply-container{
  background-color: #f1f1f1;
  display: flex;
  flex-direction: column;
  height: 100%;
}



.page-head{
  font-size: 13px;
  line-height: 18px;
  margin-left: 21px;
  display: block;
  padding-top: 20px;
  margin-bottom: 14px;
}
.scroll-container{
  flex: 1;
  overflow: scroll;
  background-color: #fff;
}
.page-footer{
  background-color: #fff;
  padding-top: 10px;
  height: 60px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.form-section{
  margin-top: 14px;
  margin-left: 24px;
  margin-right: 24px;
}
.form-label{
  font-size: 11px;
}
.weui-input{
  border: 1px solid #f1f1f1;
  border-radius: 4px;
  padding-left: 17px;
  line-height: 40px;
  font-size: 11px;
  margin-top: 10px;
}
.check-area{
  width: 40px;
  font-size: 11px;
  margin-top: 24px;
}
.des_text{
  margin-top: 0px;
  margin-left: 24px;
  margin-right: 24px;
  color: #9e9e9e;
  font-size: 11px;
}

